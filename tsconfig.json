{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "declaration": true

  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": ["src/**/*"]
}
